<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>parentGroupId</groupId>
        <artifactId>parentArtifactId</artifactId>
        <version>parentVersion</version>
    </parent>
    <packaging>jar</packaging>
    <name>set version for pom with no dependencies</name>
    <properties>
        <propertyA>propertyAValue</propertyA>
        <projectVersion>doNotOverride</projectVersion>
    </properties>
    <dependencies>
        <dependency>
            <groupId>${propertyA}</groupId>
            <version>version</version>
            <artifactId>artifactA</artifactId>
        </dependency>
    </dependencies>
    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>groupB</groupId>
                <version>versionB</version>
                <artifactId>artifactB</artifactId>
            </dependency>
        </dependencies>
    </dependencyManagement>
    <profiles>
        <profile>
            <id>profileA</id>
            <dependencies>
                <dependency>
                    <groupId>groupC</groupId>
                    <artifactId>artifactC</artifactId>
                    <version>versionC</version>
                </dependency>
            </dependencies>
            <dependencyManagement>
                <dependencies>
                    <dependency>
                        <groupId>groupD</groupId>
                        <artifactId>artifactD</artifactId>
                        <version>versionD</version>
                    </dependency>
                </dependencies>
            </dependencyManagement>
        </profile>
    </profiles>
</project>
